{
  "name": "scared-ape",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "build": "tsc",
    "boot": "pnpm build:swc && node dist/cli/boot.js",
    "boot:dev": "pnpm build:swc && NODE_ENV=dev node dist/cli/boot.js",
    "dev:watch": "pnpm build:swc && (pnpm watch:swc & NODE_ENV=dev nodemon dist/index.js)",
    "pre": "pnpm lint & pnpm type-check",
    "test": "vitest run",
    "dev": "pnpm build:swc && NODE_ENV=dev node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf dist config/local-* config/*dev*",
    "lint:fix": "eslint . --ext .ts --fix",
    "build:swc": "swc src -d dist",
    "watch:swc": "swc src -d dist -w"
  },
  "keywords": [],
  "author": "rezonmain",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.66",
    "@types/better-sqlite3": "^7.6.4",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^9.0.3",
    "@types/lodash": "^4.14.195",
    "@types/node-fetch": "2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "add": "^2.0.6",
    "better-sqlite3": "^8.4.0",
    "config": "^3.3.9",
    "express": "^4.18.2",
    "grammy": "^1.17.1",
    "inquirer": "^9.2.7",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "puppeteer": "^20.7.3",
    "redis": "^4.6.7",
    "toad-scheduler": "^3.0.0",
    "zod": "^3.21.4"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true,
    "rules": {
      "@typescript-eslint/consistent-type-imports": "error",
      "@typescript-eslint/no-namespace": "off"
    }
  }
}
